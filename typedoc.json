{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages", 
  "entryPoints": ["packages/*"],
  "plugin": ["typedoc-plugin-markdown"],

  "name": "Trailhead API Documentation",
  "includeVersion": true,
  "readme": "none",
  "projectDocuments": [],
  "includeHierarchySummary": false,
  "searchInDocuments": false,
  "searchInComments": false,

  "packageOptions": {
    "entryPoints": ["src/index.ts"],
    "includeVersion": false,
    "readme": "none",
    "projectDocuments": [],
    "skipErrorChecking": true,
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "excludeNotDocumented": false,
    "excludeReferences": true,
    "disableSources": true,
    "exclude": ["**/README.md", "**/docs/**", "**/*.md", "**/*.test.ts", "**/*.spec.ts"]
  },

  "flattenOutputFiles": true,
  "mergeReadme": false,
  "entryFileName": "README",
  "router": "member",
  "membersWithOwnFile": [],
  "fileExtension": ".md",
  "hideBreadcrumbs": false,
  "hidePageHeader": false,
  "hidePageTitle": false,
  "router": "member",

  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false,
    "rewrittenLink": true,
    "unusedMergeModuleWith": true
  },

  "requiredToBeDocumented": ["Class", "Interface", "Function", "Method"],

  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": false,
    "compactFolders": true,
    "excludeReferences": true
  },

  "categorizeByGroup": false,
  "defaultCategory": "API",

  "hideGenerator": true,
  "githubPages": false,
  "skipErrorChecking": false,
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "disableSources": true,
  "excludeReferences": true,

  "sort": ["source-order"],

  "blockTags": [
    "@alpha",
    "@beta",
    "@defaultValue",
    "@decorator",
    "@deprecated",
    "@eventProperty",
    "@example",
    "@experimental",
    "@inheritDoc",
    "@internal",
    "@label",
    "@override",
    "@packageDocumentation",
    "@param",
    "@privateRemarks",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@sealed",
    "@see",
    "@since",
    "@throws",
    "@typeParam",
    "@virtual"
  ],

  "inlineTags": ["@link", "@inheritDoc", "@label"],

  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeCategories": ["Namespace", "Re-exports"],
  "excludeExternals": true,
  "excludeTags": ["@namespace"],
  "excludeReferences": true,
  "externalPattern": ["node_modules/**/*", "**/node_modules/**/*"],
  "intentionallyNotExported": [
    "z", "ok", "err", "Result", "CoreResult", "CoreResultAsync",
    "fromPromiseAsync", "fromThrowableAsync", "fromThrowableAsyncFunc"
  ],

  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false,
    "@alpha": false,
    "@beta": true,
    "@internal": false
  },

  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "excludeNotDocumented": false,
  "excludeNotDocumentedKinds": [
    "Constructor", "Property", "Method", "Accessor", 
    "GetSignature", "SetSignature", "CallSignature", "IndexSignature"
  ],

  "externalSymbolLinkMappings": {
    "typescript": {
      "Promise": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Promise",
      "Partial": "https://www.typescriptlang.org/docs/handbook/utility-types.html#partialtype",
      "Required": "https://www.typescriptlang.org/docs/handbook/utility-types.html#requiredtype",
      "Readonly": "https://www.typescriptlang.org/docs/handbook/utility-types.html#readonlytype"
    },
    "global": {
      "Promise": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Promise",
      "Array": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array",
      "Object": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Object"
    }
  },

  "searchCategoryBoosts": {
    "Classes": 1.5,
    "Interfaces": 1.5,
    "Functions": 1.3
  },

  "markdownItOptions": {
    "html": false,
    "linkify": true
  },

  "publicPath": "",
  "sanitizeComments": true,

  "preserveWatchOutput": true
}
