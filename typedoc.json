{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "entryPointStrategy": "packages",
  "entryPoints": ["packages/*"],
  "out": "docs",
  "mergeReadme": true,
  "outputFileStrategy": "modules",
  "flattenOutputFiles": true,
  "fileExtension": ".md",
  "entryFileName": "README.md",
  "membersWithOwnFile": [],

  "name": "Trailhead API Documentation",
  "includeVersion": true,
  "readme": "none",

  "packageOptions": {
    "entryPoints": ["src/index.ts"],
    "includeVersion": false,
    "readme": "none",
    "skipErrorChecking": false,
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "excludeNotDocumented": false,
    "excludeReferences": true,
    "disableSources": true,
    "exclude": ["**/README.md", "**/docs/**", "**/*.md", "**/*.test.ts", "**/*.spec.ts"]
  },

  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": true,
    "rewrittenLink": true,
    "unusedMergeModuleWith": true
  },

  "requiredToBeDocumented": ["Class", "Interface", "Function", "Method"],

  "navigation": {
    "includeCategories": false,
    "includeGroups": false,
    "includeFolders": false,
    "compactFolders": true,
    "excludeReferences": true
  },

  "categorizeByGroup": false,
  "defaultCategory": "API",

  "hideGenerator": true,
  "githubPages": false,
  "skipErrorChecking": true,
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "logLevel": "Error",
  "disableSources": true,
  "excludeReferences": true,

  "sort": ["source-order"],

  "blockTags": [
    "@alpha",
    "@author",
    "@beta",
    "@defaultValue",
    "@decorator",
    "@deprecated",
    "@description",
    "@eventProperty",
    "@example",
    "@experimental",
    "@inheritDoc",
    "@internal",
    "@label",
    "@override",
    "@packageDocumentation",
    "@param",
    "@privateRemarks",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@sealed",
    "@see",
    "@since",
    "@throws",
    "@typeParam",
    "@virtual"
  ],

  "inlineTags": ["@link", "@inheritDoc", "@label"],

  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeCategories": ["Namespace", "Re-exports"],
  "excludeExternals": true,
  "excludeTags": ["@namespace"],

  "externalPattern": ["node_modules/**/*", "**/node_modules/**/*"],
  "intentionallyNotExported": [
    "CommandArgument",
    "FileSystem",
    "Logger",
    "CommandConfig",
    "CommandOptions",
    "ConfigOperations",
    "ValidatorOperations",
    "ConfigResult",
    "ManagerDependencies",
    "FormatterOptions",
    "ConfigSchema",
    "FileCategory",
    "ConversionQuality",
    "ConversionOptions",
    "CheckConversionOp",
    "GetSupportedFormatsOp",
    "ValidateStringOperation",
    "DetectFromBufferOp",
    "DetectFromExtensionOp",
    "DetectFromFileOp",
    "FileFormatInfo",
    "DetectionReliability",
    "DetectionSource",
    "ValidateBufferOperation",
    "GetExtensionsOp",
    "GetMimeTypeOp",
    "IsMimeTypeOp",
    "ParsedData",
    "FindFilesOp",
    "SortOptions",
    "loadTemplateFilesFromDirectory"
  ],

  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false,
    "@alpha": false,
    "@beta": true,
    "@internal": false
  },

  "exclude": ["**/node_modules/**", "**/*.test.ts", "**/*.spec.ts"],
  "excludeNotDocumented": false,
  "excludeNotDocumentedKinds": [
    "Constructor",
    "Property",
    "Method",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "CallSignature",
    "IndexSignature"
  ],

  "externalSymbolLinkMappings": {
    "typescript": {
      "Promise": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Promise",
      "Partial": "https://www.typescriptlang.org/docs/handbook/utility-types.html#partialtype",
      "Required": "https://www.typescriptlang.org/docs/handbook/utility-types.html#requiredtype",
      "Readonly": "https://www.typescriptlang.org/docs/handbook/utility-types.html#readonlytype"
    },
    "global": {
      "Promise": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Promise",
      "Array": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Array",
      "Object": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/Object"
    },
    "@trailhead/create-cli": {
      "loadTemplateFilesFromDirectory": "#"
    }
  },

  "searchCategoryBoosts": {
    "Classes": 1.5,
    "Interfaces": 1.5,
    "Functions": 1.3
  },

  "preserveWatchOutput": true
}
