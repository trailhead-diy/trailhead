{
  "name": "@trailhead/sort",
  "version": "1.0.1",
  "description": "Fast, type-safe sorting utilities powered by es-toolkit",
  "keywords": [
    "sort",
    "es-toolkit",
    "typescript",
    "performance"
  ],
  "type": "module",
  "private": false,
  "author": "Esteban Rojas (Trailhead DIY)",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean && tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "format": "prettier --write . --log-level=error --ignore-path .prettierignore",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "types": "tsc --noEmit",
    "prepublishOnly": "echo \"Publishing without rebuild\""
  },
  "dependencies": {
    "es-toolkit": "^1.41.0"
  },
  "devDependencies": {
    "@repo/prettier-config": "workspace:*",
    "@repo/tsup-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "oxlint": "^1.25.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "prettier": "@repo/prettier-config",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trailhead-diy/trailhead",
    "directory": "packages/sort"
  },
  "bugs": {
    "url": "https://github.com/trailhead-diy/trailhead/issues"
  },
  "homepage": "https://github.com/trailhead-diy/trailhead#readme",
  "publishConfig": {
    "access": "public"
  }
}
