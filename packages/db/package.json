{
  "name": "@esteban-url/db",
  "version": "1.0.0",
  "description": "Functional database operations for CLI applications",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "default": "./dist/adapters/index.js"
    },
    "./query": {
      "types": "./dist/query/index.d.ts",
      "default": "./dist/query/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean && tsup && tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@esteban-url/core": "workspace:*",
    "@esteban-url/validation": "workspace:*",
    "@esteban-url/fs": "workspace:*"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "database",
    "sql",
    "query-builder",
    "sqlite",
    "functional-programming",
    "cli",
    "typescript"
  ],
  "author": "Esteban Borai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esteban-url/trailhead-cli.git",
    "directory": "packages/db"
  }
}
