{
  "name": "@trailhead/cli",
  "version": "0.1.0",
  "type": "module",
  "private": false,
  "description": "Private CLI framework for Trailhead projects - requires GitHub installation",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@trailhead:registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./command": {
      "types": "./dist/command/index.d.ts",
      "default": "./dist/command/index.js"
    },
    "./filesystem": {
      "types": "./dist/filesystem/index.d.ts",
      "default": "./dist/filesystem/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./prompts": {
      "types": "./dist/prompts/index.d.ts",
      "default": "./dist/prompts/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:prod": "NODE_ENV=production tsup && tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "test": "pnpm build && vitest run",
    "test:coverage": "pnpm build && vitest run --coverage",
    "test:watch": "vitest",
    "test:examples": "./examples/test-examples.sh",
    "test:examples:fast": "./examples/test-examples.sh --fast",
    "test:all": "pnpm test && pnpm test:examples",
    "lint": "oxlint",
    "types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "pretest": "pnpm build",
    "prepack": "pnpm build",
    "validate": "pnpm types && pnpm lint && pnpm test:all"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.0",
    "ora": "^8.0.1",
    "p-retry": "^6.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/oxlint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^9.0.0",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^3.0.0",
    "oxlint": "^1.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cli",
    "framework",
    "command-line",
    "typescript",
    "internal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esteban-url/trailhead.git",
    "directory": "packages/cli"
  },
  "license": "MIT"
}
