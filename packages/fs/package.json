{
  "name": "@esteban-url/fs",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "description": "Functional filesystem operations with Result types for Trailhead System",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@esteban-url:registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "types": "tsc --noEmit",
    "format": "prettier --write . --log-level=error --ignore-path .prettierignore ",
    "clean": "rm -rf dist",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@esteban-url/core": "workspace:*",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "oxlint": "^1.9.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "filesystem",
    "fs",
    "files",
    "functional",
    "result-types",
    "typescript",
    "trailhead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esteban-url/trailhead.git",
    "directory": "packages/fs"
  },
  "license": "MIT"
}
