{
  "name": "@trailhead/data",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "description": "Functional data processing (CSV, JSON, Excel) with Result types for Trailhead System",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@trailhead:registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./csv": {
      "types": "./dist/csv/index.d.ts",
      "default": "./dist/csv/index.js"
    },
    "./json": {
      "types": "./dist/json/index.d.ts",
      "default": "./dist/json/index.js"
    },
    "./excel": {
      "types": "./dist/excel/index.d.ts",
      "default": "./dist/excel/index.js"
    },
    "./streaming": {
      "types": "./dist/streaming/index.d.ts",
      "default": "./dist/streaming/index.js"
    },
    "./streaming/csv": {
      "types": "./dist/streaming/csv-streaming.d.ts",
      "default": "./dist/streaming/csv-streaming.js"
    },
    "./streaming/json": {
      "types": "./dist/streaming/json-streaming.d.ts",
      "default": "./dist/streaming/json-streaming.js"
    },
    "./streaming/excel": {
      "types": "./dist/streaming/excel-streaming.d.ts",
      "default": "./dist/streaming/excel-streaming.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsup && tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "types": "tsc --noEmit",
    "format": "prettier --write . --ignore-path .prettierignore",
    "clean": "rm -rf dist",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@trailhead/core": "workspace:*",
    "@trailhead/fs": "workspace:*",
    "papaparse": "^5.4.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "peerDependencies": {
    "@trailhead/streams": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@trailhead/streams": {
      "optional": true
    }
  },
  "devDependencies": {
    "@repo/oxlint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/node": "^24.0.10",
    "@types/papaparse": "^5.3.14",
    "@vitest/coverage-v8": "^3.2.4",
    "oxlint": "^1.6.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "data",
    "csv",
    "json",
    "excel",
    "processing",
    "functional",
    "result-types",
    "typescript",
    "trailhead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esteban-url/trailhead.git",
    "directory": "packages/data"
  },
  "license": "MIT"
}
