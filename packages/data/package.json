{
  "name": "@trailhead/data",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "description": "Unified data processing and format detection (CSV, JSON, Excel) with Result types for Trailhead System",
  "author": "Esteban Rojas (Trailhead DIY)",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./csv": {
      "types": "./dist/csv/index.d.ts",
      "default": "./dist/csv/index.js"
    },
    "./json": {
      "types": "./dist/json/index.d.ts",
      "default": "./dist/json/index.js"
    },
    "./excel": {
      "types": "./dist/excel/index.d.ts",
      "default": "./dist/excel/index.js"
    },
    "./formats": {
      "types": "./dist/formats/index.d.ts",
      "default": "./dist/formats/index.js"
    },
    "./formats/detection": {
      "types": "./dist/detection/index.d.ts",
      "default": "./dist/detection/index.js"
    },
    "./formats/mime": {
      "types": "./dist/mime/index.d.ts",
      "default": "./dist/mime/index.js"
    },
    "./formats/conversion": {
      "types": "./dist/conversion/index.d.ts",
      "default": "./dist/conversion/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "types": "tsc --noEmit",
    "format": "prettier --write . --log-level=error --ignore-path .prettierignore ",
    "clean": "rm -rf dist",
    "prepublishOnly": "echo \"Publishing without rebuild\""
  },
  "dependencies": {
    "@trailhead/cli": "workspace:*",
    "@trailhead/core": "workspace:*",
    "file-type": "^21.0.0",
    "mime-types": "^3.0.1",
    "papaparse": "^5.4.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@repo/tsup-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/vitest-config": "workspace:*",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.0",
    "@types/papaparse": "^5.5.2",
    "@vitest/coverage-v8": "^4.0.16",
    "oxlint": "^1.35.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "data",
    "csv",
    "json",
    "excel",
    "formats",
    "detection",
    "mime",
    "processing",
    "functional",
    "result-types",
    "typescript",
    "trailhead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trailhead-diy/trailhead.git",
    "directory": "packages/data"
  },
  "bugs": {
    "url": "https://github.com/trailhead-diy/trailhead/issues"
  },
  "homepage": "https://github.com/trailhead-diy/trailhead#readme",
  "license": "MIT"
}
