  "searchCategoryBoosts": {
    "Classes": 1.5,
    "Interfaces": 1.5,
    "Functions": 1.3
  },

  "markdownItOptions": {
    "html": true,
    "linkify": true
  },

  "preserveWatchOutput": true,

  "gitRevision": "main",
  "gitRemote": "origin",
  "sourceLinkExternal": true
}
